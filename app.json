{
  "expo": {
    "name": "Goaliath",
    "slug": "Goaliath",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#536BAC"
    },
    "splash": {
      "image": "./assets/splash-icon-color.png",
      "resizeMode": "contain",
      "backgroundColor": "#536BAC"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon-ios.png",
      "bundleIdentifier": "app.goaliath"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-crown-smaller.png",
        "backgroundColor": "#536BAC"
      },
      "package": "app.goaliath",
      "versionCode": 10
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "jsEngine": "hermes",
    "plugins": [
      "sentry-expo",
      "expo-localization"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "goaliath",
            "project": "goaliath",
            "authToken": "265a7966561944468d17d179fa676d4338efaaf6f22f469a9dab011a64026909",
            "setCommits": true
          }
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "a81da208-c572-41f9-b40b-7e7195551825"
      }
    }
  }
}
